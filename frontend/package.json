{
	"name": "appstract",
	"version": "1.6.0",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"prestart": "npm run bump",
		"start": "ng serve --proxy-config utils/proxy.js",
		"start:noproxy": "ng serve --verbose=true --preserve-symlinks",
		"build": "ng build",
		"bump": "node utils/version",
		"build:stag": "ng lint && ng build -c stag",
		"build:lab": "npm run bump && ng lint && ng build -c stag --base-href \"/appstract\"/",
		"build:prod": "npm run bump && ng lint && ng build -prod",
		"test": "ng test",
		"lint": "ng lint --format stylish",
		"lint:fix": "ng lint --format stylish --fix",
		"e2e": "ng e2e",
		"docs:gen": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json --theme postmark --includes ./externaldocs",
		"docs:serve": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json --serve --theme postmark --includes ./externaldocs",
		"docs:watch": "./node_modules/.bin/compodoc -p src/tsconfig.compodoc.json --serve --watch --theme postmark --includes ./externaldocs",
		"docs": "npm run docs:gen && cd ./documentation && http-server",
		"reinstall": "rm -rf ./node_modules/ && npm install",
		"reinstall:hard": "rm -rf ./package-lock.json && rm -rf ./node_modules/ && npm install",
		"reboot": "npm run reinstall && npm start",
		"reboot:prod": "npm run reinstall && ng build -prod",
		"ls": "ls ./../ -lah",
		"swagger:gen": "java -jar ./../swagger-codegen-cli-2.2.3.jar generate -l typescript-angular2 -i https://lab3.itcraft.pl/appstract-api/swagger-api-json -o swagger",
		"swagger:copy": "npm run swagger:gen && rm -r ./src/app/_interfaces/models/* && cp ./swagger/model/* ./src/app/_interfaces/models/ && rm -rf ./swagger/"
	},
	"private": true,
	"dependencies": {
		"@agm/core": "^1.0.0-beta.2",
		"@angular-devkit/core": "^0.4.2",
		"@angular-devkit/schematics": "^0.4.2",
		"@angular/animations": "^6.1.8",
		"@angular/common": "^6.1.8",
		"@angular/compiler": "^6.1.8",
		"@angular/core": "^6.1.8",
		"@angular/forms": "^6.1.8",
		"@angular/http": "^6.1.8",
		"@angular/platform-browser": "^6.1.8",
		"@angular/platform-browser-dynamic": "^6.1.8",
		"@angular/router": "^6.1.8",
		"@fortawesome/angular-fontawesome": "^0.3.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.8",
		"@fortawesome/free-regular-svg-icons": "^5.5.0",
		"@fortawesome/free-solid-svg-icons": "^5.5.0",
		"@ng-bootstrap/ng-bootstrap": "^3.2.0",
		"@ng-select/ng-select": "^2.14.0",
		"@ngx-progressbar/core": "^5.1.2",
		"@ngx-progressbar/http": "^5.1.2",
		"@ngx-translate/core": "^10.0.0",
		"@ngx-translate/http-loader": "^3.0.0",
		"@swimlane/ngx-datatable": "^13.1.0",
		"@wuja/logger": "^2.0.0",
		"bootstrap": "^4.1.0",
		"classlist.js": "^1.1.20150312",
		"core-js": "^2.4.1",
		"font-awesome": "^4.7.0",
		"ng-connection-service": "^1.0.4",
		"ngx-infinite-scroll": "^0.8.2",
		"ngx-toastr": "^9.1.0",
		"rxjs": "^6.3.2",
		"rxjs-compat": "^6.0.0-rc.0",
		"web-animations-js": "^2.3.1",
		"zone.js": "^0.8.18"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.6.8",
		"@angular/cli": "^6.2.3",
		"@angular/compiler-cli": "^6.1.8",
		"@angular/language-service": "^6.1.8",
		"@compodoc/compodoc": "^1.0.7",
		"@types/jasmine": "~2.5.53",
		"@types/jasminewd2": "~2.0.2",
		"@types/lodash": "^4.14.78",
		"@types/node": "^6.0.90",
		"codelyzer": "^4.4.4",
		"git-describe": "^4.0.3",
		"jasmine-core": "~2.6.2",
		"jasmine-spec-reporter": "~4.1.0",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.1.1",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"lodash": "^4.17.4",
		"moment": "^2.19.1",
		"protractor": "~5.1.2",
		"ts-node": "~3.2.0",
		"tslint": "^5.9.1",
		"typescript": "~2.9.2"
	}
}
